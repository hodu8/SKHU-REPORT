package hw5_1;

public class Driver {

	public static void main(String[] args) {
		Member [] mem = new Member[5];
		for (int i=0; i<5; i++)
			mem[i] = new Member();
		for (int j=0; j<5; j++)
			mem[j].memberRank(j+1);
		for (int k=0; k<5; k++)
			mem[k].memberPoint();
		for (int p=0; p<5; p++)
			mem[p].memberPoint();
		for (int l=0; l<5; l++)
			System.out.println(mem[l].discountPercent());
	}
}
