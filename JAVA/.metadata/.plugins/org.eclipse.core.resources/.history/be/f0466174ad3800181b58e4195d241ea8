package lab5_4;

public class PointTest {

	public static void main(String[] args) {
		System.out.println("lab5_4: 박찬우");
		Point p1 = new Point();
		Point p2 = new Point();
		p1.move(4.5);
		p2.move(-1.0);
		p1.move(1.0);
		p1.move();
		System.out.println(p1.toString());
		System.out.println(p2.toString());
		System.out.println(p1.distance(p2));
	}

}
