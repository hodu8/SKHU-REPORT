package example;

public class example {
	public static void main(String[] args) {
		int result, x = 6, y = 0;
		try {
			System.out.println("a");
			result = x/y;
			System.out.println('b');
		} catch (ArrayIndexOutOfBoundsException e) {
			result = 1;
			System.out.println('c');
		} finally {
			System.out.println("d");
		}
		System.out.println(result);
	}
}