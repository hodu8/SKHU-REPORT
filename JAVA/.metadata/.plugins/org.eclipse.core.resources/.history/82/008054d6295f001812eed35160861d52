package hw9_1;

public class Person implements Speakable{
	String name;
	public Person(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	@Override
	public void speak() {
		System.out.print("안녕하세요\n" + name + "입니다.\n");
	}
}