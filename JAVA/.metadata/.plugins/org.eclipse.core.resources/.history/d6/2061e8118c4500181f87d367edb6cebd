package hw6_2;

public class BankAccount {
	private int accountnum = 0;
	private String accountname;
	private int money = 0;
	public BankAccount(int accnum, String accname, int m) {
		accountnum = accnum;
		accountname = accname;
		if (m<0)
			money = 0;
		else 
			money = m;
	}
	public BankAccount () {
		this(this.accountnum, this.accountname);
	}
	
}
